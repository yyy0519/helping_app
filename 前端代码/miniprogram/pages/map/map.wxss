page{
	background-color: #f4f4f4;
	padding-bottom: 48rpx;
}
map{
	width: 100%;
	height: 640rpx;
}
.blank{
    height:10px;
}
.fixed_model{
	position: relative;
	background: #FFFFFF;
	box-shadow: 0 4rpx 32rpx -4rpx rgba(0,0,0,0.14), 0 4rpx 4rpx -4rpx rgba(0,0,0,0.13);
	border-radius: 16rpx;
	padding: 30rpx 40rpx;
	width: 610rpx;
	height: 108rpx;
	margin: -84rpx auto 0;
}
.fixed_model .title{
	font-size: 34rpx;
	color: var(--mb-color);
}
.fixed_model .in{
	font-size: 34rpx;
	margin-top: 12rpx;
	color: var(--mb-color);
    font-weight: 300;
    width: 502rpx; 
    height: 46rpx; 
    display: block; 
    box-sizing: border-box;
}
.fixed_model .btn{
	position: absolute;
	background: #F0F0F0;
	width: 96rpx;
	height: 96rpx;
	right: 30rpx;
	top: 36rpx;
	border-radius: 50%;
	display: flex;
}
.btn image{
	width: 50%;
	height: 50%;
	margin: auto;
}
.change_model{
	background: #FFFFFF;
	box-shadow: 0 4rpx 30rpx -4rpx rgba(0,0,0,0.14), 0 4rpx 4rpx -4rpx rgba(0,0,0,0.13);
	border-radius: 10rpx;
	width: 202rpx;
	height: 48rpx;
	display: flex;
	padding: 10rpx;
	position: absolute;
    top: 469rpx;
    right: 30rpx;
}
.change_model image{
	width: 48rpx;
	height: 48rpx;
}
.change_model view{
	font-size: 28rpx;
	color: var(--mb-color);
	letter-spacing: 0;
	text-align: right;
	font-weight: 400;
	margin: auto;
}
.info_model{
	background-color: #FFFFFF;
	width: 690rpx;
	margin: 32rpx auto;
	border-radius: 16rpx;
}
.info_model .info_item{
	padding: 40rpx;
	min-height: 48rpx;
	display: flex;
}
.info_model .info_item view{
	font-size: 32rpx;
	color: var(--lb-color);
	width: 35%;
}
.info_model .info_item view+view{
	font-size: 32rpx;
	color: #000000;
	width: 65%;
	text-align: right;
}
.info_model .info_item+.info_item{
	box-shadow: inset 0 2rpx 0 0 rgba(224,224,224,0.50);
}
.img{
    width:40px; 
    height: 40px;
    border-radius: 30px;
}
.xinxi{
    display: flex;
    flex-direction: row;
    margin-left: 5px;
}
.scroll_view_vertical{
    height: 100%;
    margin-bottom: 20px;
    
}
.box{
    height:180px;
    box-shadow: #ccc  0 0 0rpx 0rpx;
    color: rgb(106, 106, 106);
    border: 2rpx solid #ccc;
    border-radius: 10rpx;
    background-color: rgb(255, 255, 255);
    margin-left: 5px;
    margin-right: 5px;
    padding: 5px;
}
.loc-end{
    display: flex;
    justify-content: space-between;
    margin-top: 5px;
    margin-right: 5px;
}
.title{
   margin-top: -15px;
   margin-bottom: 5px;
    font-size: 21px;
    color: black;
    font-weight: bold;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis; 
}
.item{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis; 
    margin-left:0px;
    margin-top: 0px;
    margin-bottom: 15px;
}
.detail{
    margin-left: 5px;
    margin-right: 5px;
    margin-top: -12px;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
    text-overflow: ellipsis; 
}

.nicheng{
    position: relative;
    left:13px;
    font-size: 16px;
    color: black;
}
.date{
    width: 344rpx; 
    height: 42rpx; 
    display: block; 
    position: relative; 
    left: 105rpx; 
    top: -20px;
    margin-left: 5px;
}
.tiptail{
    margin-right: 5px;
    margin-left: 2px;
    margin-top: 0px;
    margin-bottom: 5px;
}
.sorttxt{
    margin-left: 18px;
    color:#402c47;
    font-size: 15px;
}
.sortbtn{ 
   font-size: 17px;
   position: relative; 
   right:91px;
   border: 1px solid #563b61; 
   padding:4px;
   color:#423049;
   background-color:#fdfdfd ;
   margin-top: 5px;
}
.sort{
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    position: relative;
    padding:10px;
    border: 1px solid #563b61; 
    margin-left: 8px;
    margin-right: 8px;
    border-radius: 8px;
    background-color: #fff;
}
.icon{
    width:19px;
    height:19px;
    position: relative;
    right:4px;
    top:3px;
    
    
}